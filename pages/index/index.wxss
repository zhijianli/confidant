 .page{
  width: 100%;
  height: 100%;
} 
.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.userinfo-nickname {
    color: #aaa;
}

.usermotto {
    margin-top: 200px;
}

.classify {
    width: 100%;
    display: flex;
    justify-content: space-around;
    height: 188rpx;
    padding: 36rpx 0;
    margin-bottom: 24rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.wx-li {
    min-width: 96rpx;
    padding-top: 92rpx;
    color: #333;
    text-align: center;
    font-size: 24rpx;
    background-position: top;
    background-repeat: no-repeat;
    background-size: 80rpx 80rpx;
}

.choice-testing, .hot-testing {
    /* margin-bottom: 24rpx; */
    margin-top: 30rpx;
    background-color: #fff;
    width: 100%;
}

.testing-title {
    color: #4A4A4A;
    /* text-align: center; */
    margin-left: 3.2%;
    font-size: 30rpx;
    line-height: 92rpx;
    /* font-weight:bold; */

}

