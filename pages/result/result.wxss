.to-reply-input-wrapper {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 750rpx;
    padding: 24rpx;
    position: fixed;
    bottom: 0;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
    box-sizing: border-box;
    background-color: #fff;
    padding-bottom: 0rpx;
    z-index: 1;
}

.to-reply-input {
    flex: 1;
    max-height: 200rpx;
    min-height: 50rpx;
    margin-right: 20rpx;
    font-size: 30rpx;
    text-align: left;
    border: none;
    resize: none;
    outline: none;
    -webkit-user-modify: read-write-plaintext-only;
    box-sizing: border-box;
    line-height: 1.5;
    word-break: keep-all;
    word-wrap: break-word;
    overflow: hidden;
    /* overflow-y: auto; */
    border-bottom: 1px solid #999;
    margin-bottom: 24rpx;
    list-style: none;
}

.to-reply-input::before {
    content: '我来说说...';
    display: none;
    color: #999;
}

.to-reply-input:empty::before {
    display: block;
}

.to-reply-input::placeholder {
    color: #999;
}

.to-reply-input.c-actived::before {
    display: none;
}

.send-comment-btn {
    display: block;
    color: #333;
    font-size:30rpx;

}

.comment-loading {
