 .page{
  width: 100%;
  overflow: hidden;
} 
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0  0;
  box-sizing: border-box;
}  

.result-box {
    margin: 20rpx auto;
    background-color: #fff;
}

.result-title {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 30rpx;
    /* color: #333; */
   color: #00d3dd;  
    font-size: 30rpx;
    line-height: 50rpx;
}

.pei-image {
    width: 100%;
    object-fit: cover;
}

.pei-image image {
    width: 100%;
}

.result-chart {
    display: flex;
    padding-top: 48rpx;
    padding-bottom: 40rpx;
}

.score {
    display: flex;
    justify-content: space-between;
    flex-direction: column-reverse;
    height: calc(484rpx + 20px);
    margin: -15rpx 20rpx 0rpx 0rpx;
    /* color: #bfbfbf; */
   color: #4A4A4A;
    text-align: center;
    font-size: 28rpx;
     /* float: left;  */
    /* padding-right: 15rpx;
         border-right: 3.7rpx solid #dddddd; */
 
}



.show-score, .show-lat {
    width: 250rpx;
    min-height: 110rpx;
    padding: 15rpx 50rpx;
    position: fixed;
    z-index: 9999;
    box-sizing: border-box;
    color: #fff;
    text-align: center;
    font-size: 40rpx;
    border-radius: 6px;
    background-color: #666;
}

.show-score .sp {
    display: block;
    /* width: 1.45rem; */
    margin: 10rpx auto 0;
    font-size: 24rpx;
}

.show-score::after {
    content: "";
    width: 20rpx;
    height: 20rpx;
    position: absolute;
    left: 50%;
    bottom: -9rpx;
    transform: translateX(-50%) rotate(45deg);
    background: linear-gradient(135deg, transparent 50%, #666 50%);
}

.show-lat::after {
    content: "";
    width: 20rpx;
    height: 20rpx;
    position: absolute;
    left: 50%;
    bottom: -9rpx;
    transform: translateX(-50%) rotate(45deg);
    background: linear-gradient(-45deg, transparent 50%, #666 50%);
}

.show-lat {
    display: flex;
    padding: 0;
    word-wrap: break-word;
    top: auto;
}

.show-lat .sp {
    display: block;
    margin: auto;
    font-size: 30rpx;
    line-height: 1.5;
}

.result-chart-content {
    flex: 1;
    height: 570rpx;
    position: relative;
}

.result-chart-content .zhu-wrapper {
    width: 100%;
    height: 100%;
    position: relative;
    /* 滚动条 */
    overflow: auto;
}

.result-chart-content .zhu-wrapper::-webkit-scrollbar {
    display: none;
}

.result-chart-content .result-ul {
    display: flex;
    align-items: flex-end;
    padding-left: 40rpx;
    position: absolute;
    bottom: 0;
}

.result-chart-content .result-ul::after {
    content: "";
    padding-left: 1px;
    height: 1px;
}

.result-chart-content .result-li {
    flex-shrink: 0;
    margin-right: 40rpx;
    position: relative;
}

.result-chart-content .result-li .zhu {
    width: 40rpx;
    height: 60rpx;
    margin: 0 auto;
    /* background: linear-gradient(to bottom, #66a6ff 3%, #afe1ff); */
      border-radius: 3px; 
}

.result-chart-content .result-li .result-p {
    /* width: 56px; */
    max-width: 140rpx;
    height: 68rpx;
    margin-top: 20rpx;
    font-size: 28rpx;
    color: #333;
    line-height: 1.15;
}

.result-chart-content .result-li:last-child {
    margin-right: 130rpx;
}

.result-chart-line-wrapper {
    display: flex;
    justify-content: space-between;
    flex-direction: column;
    width: 100%;
    height: 484rpx;
    position: absolute;
    top: 0;
    box-sizing: border-box;
    border-left: 1px solid #f8f8f8;
}

.line {
